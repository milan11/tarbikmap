{
  // Use IntelliSense to find out which attributes exist for C# debugging
  // Use hover for the description of the existing attributes
  // For further information visit https://github.com/OmniSharp/omnisharp-vscode/blob/master/debugger-launchjson.md
  "version": "0.2.0",
  "configurations": [
    {
      "name": "data_getting_wiki_data",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build_data_getting_wiki_data",
      "program": "${workspaceFolder}/TarbikMap.DataGetting.WikiData/bin/Debug/netcoreapp5.0/TarbikMap.DataGetting.WikiData",
      "args": [],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "console": "internalConsole"
    },
    {
      "name": "data_getting_wiki_data_classes",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build_data_getting_wiki_data_classes",
      "program": "${workspaceFolder}/TarbikMap.DataGetting.WikiDataClasses/bin/Debug/netcoreapp5.0/TarbikMap.DataGetting.WikiDataClasses",
      "args": [],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "console": "internalConsole"
    },
    {
      "name": "tests_browser_local",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build_tests_browser",
      "program": "${workspaceFolder}/TarbikMap.Tests.Browser/bin/Debug/netcoreapp5.0/TarbikMap.Tests.Browser",
      "args": ["https://localhost:5001"],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "console": "internalConsole"
    },
    {
      "name": "tests_browser_azure",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build_tests_browser",
      "program": "${workspaceFolder}/TarbikMap.Tests.Browser/bin/Debug/netcoreapp5.0/TarbikMap.Tests.Browser",
      "args": ["https://tarbikmap.azurewebsites.net"],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "console": "internalConsole"
    },
    {
      "name": "tests_tasks",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build_tests_tasks",
      "program": "${workspaceFolder}/TarbikMap.Tests.Tasks/bin/Debug/netcoreapp5.0/TarbikMap.Tests.Tasks",
      "args": [],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "console": "internalConsole"
    },
    {
      "name": "web",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build_web",
      // If you have changed target frameworks, make sure to update the program path.
      "program": "${workspaceFolder}/TarbikMap/bin/Debug/netcoreapp5.0/TarbikMap.dll",
      "args": [],
      "cwd": "${workspaceFolder}/TarbikMap",
      "stopAtEntry": false,
      // Enable launching a web browser when ASP.NET Core starts. For more information: https://aka.ms/VSCode-CS-LaunchJson-WebBrowser
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "^\\s*Now listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/Views"
      }
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach",
      "processId": "${command:pickProcess}"
    }
  ]
}
